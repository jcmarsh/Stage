# single_comp.world
# A simple scenario where the robot must find the target.
# Author: James Marshall
# Date: Feb 25 2013

include "./worlds/gridcar.inc"
include "./worlds/target.inc"
include "./worlds/map.inc"
include "./worlds/floor.inc"

interval_sim  5

window
(
	size [ 900 900 ]
	scale 40

	show_data 1
)

target
(
	name "target0"
	pose [7 7 0 0]
)

gridcar
(
	name "hank"

	pose [-7 -7 0 0]
	localization_origin [0 0 0 0]
	color "blue"
)